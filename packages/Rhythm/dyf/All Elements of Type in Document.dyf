      <Script>#created for Dynamo by john pierson. based on nodes from the great Dimitar Venkov and Andreas Dieckmann
import clr
clr.AddReference('ProtoGeometry')
from Autodesk.DesignScript.Geometry import *

# Import RevitAPI
clr.AddReference("RevitAPI")
import Autodesk
from Autodesk.Revit.DB import *
from Autodesk.Revit.DB.Analysis import *

# Import DocumentManager and TransactionManager
clr.AddReference("RevitServices")
import RevitServices
from RevitServices.Persistence import DocumentManager
from RevitServices.Transactions import TransactionManager

clr.AddReference("RevitNodes")
import Revit
clr.ImportExtensions(Revit.Elements)
clr.ImportExtensions(Revit.GeometryConversion)
#Custom flatten definition to manage lists in here
def flatten(*args):
    for x in args:
        if hasattr(x, '__iter__'):
            for y in flatten(*x):
                yield y
        else:
            yield x
#The inputs to this node will be stored as a list in the IN variable.
doc = IN[0]
dataEnteringNode = IN
typelist = list()
for d in doc:
	try:
		typelist.append(FilteredElementCollector(d).OfClass(IN[1]))
	except:
		typelist.append(list())

revitTypes = flatten(typelist)
newList = list()

for i in revitTypes:
	try:
		newList.append(i.ToDSType(True))
	except:
		newList.append(list())

#Assign your output to the OUT variable.
OUT = newList, typelist</Script>
