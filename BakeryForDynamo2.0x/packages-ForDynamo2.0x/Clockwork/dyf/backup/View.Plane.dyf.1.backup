elementlist = list()
olist = list()
dlist = list()

for item in items:
	try:
		olist.append(item.Origin.ToPoint())
		dlist.append(item.ViewDirection.ToVector())
	except:
		elementlist.append(list())
OUT = (olist,dlist)</Script>
